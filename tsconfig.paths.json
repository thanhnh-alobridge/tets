{
  "compilerOptions": {
    "isolatedModules": false,
    "baseUrl": "./src",
    "paths": {
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@redux/*": ["src/redux/*"],
      "@i18n/*": ["src/i18n/*"],
      "@pages/*": ["src/pages/*"],
      "@constants/*": ["src/constants/*"],
      "@assets/*": ["src/assets/*"],
      "@/*": ["src/*"]
    }
  }
}
